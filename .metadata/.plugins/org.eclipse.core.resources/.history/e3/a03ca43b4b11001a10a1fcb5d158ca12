package com.DEStore.model;

import java.sql.Date;

public class Report {
	private int id;
	private int customerID;
	private float total;
	private Date date;
	
	public Report(int id, int customerID, float total, Date date) {
		super();
		this.id = id;
		this.customerID = customerID;
		this.total = total;
		this.date = date;
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getCustomerID() {
		return customerID;
	}
	public void setCustomerID(int customerID) {
		this.customerID = customerID;
	}
	public float getTotal() {
		return total;
	}
	public void setTotal(float total) {
		this.total = total;
	}
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}
	
	
}

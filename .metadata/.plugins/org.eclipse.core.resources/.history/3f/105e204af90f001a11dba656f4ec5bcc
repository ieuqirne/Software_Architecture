package com.DEStore.model;

import java.sql.Date;

public class Order {
	private int id;
	private int costumerID;
	private float total;
	private Date  date;
	
	public Order(int id, int costumerID, float total, Date date) {
		super();
		this.id = id;
		this.costumerID = costumerID;
		this.total = total;
		this.date = date;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getCostumerID() {
		return costumerID;
	}

	public void setCostumerID(int costumerID) {
		this.costumerID = costumerID;
	}

	public float getTotal() {
		return total;
	}

	public void setTotal(float total) {
		this.total = total;
	}

	public Date getDate() {
		return date;
	}

	public void setDate(Date date) {
		this.date = date;
	}
	
	
}

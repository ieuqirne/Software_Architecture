package com.DEStore.model;

import java.sql.Date;

public class Report {
	private String name;
	private String surname;
	private float total;
	private Date date;
	
	
	public Report(String name, String surname, float total, Date date) {
		super();
		this.name = name;
		this.surname = surname;
		this.total = total;
		this.date = date;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getSurname() {
		return surname;
	}
	public void setSurname(String surname) {
		this.surname = surname;
	}
	public float getTotal() {
		return total;
	}
	public void setTotal(float total) {
		this.total = total;
	}
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}
	
	
	
}